<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="hk.org.hkbh.cms.outpatient.model.ClinicalNoteTemplate" />
	<class name="hk.org.hkbh.cms.outpatient.model.impl.ClinicalNoteTemplateImpl" table="cms.clinical_note_template">
		<id column="id_" name="id" type="int">
			<generator class="increment" />
		</id>
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="templateTypeCodeId" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="templateName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="templateContent" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="templateContentHtml" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="updateDate" type="org.hibernate.type.TimestampType" />
		<property name="createdBy" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="updatedBy" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>